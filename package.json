{
    "name": "cardiac-arrest-react",
    "version": "1.0.0",
    "description": "An app that connects to Strava via auth0 and google maps to get a list of the highest rated hills in the google maps viewport",
    "main": "index.js",
    "scripts": {
        "test": "jest index.js",
        "start": "node api/src/server.js",
        "host": "cd ./client && npm start",
        "develop": "concurrently \"cd api/ && npm run watch\" \"cd client/ && npm start\"",
        "install": "cd api/ && npm install --force && cd ../client/ && npm install --force"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/molano1979/project3.git"
    },
    "author": "Danila Popov, Diana Cady, Ben Savage, George Pastushok",
    "license": "ISC",
    "bugs": {
        "url": "https://github.com/molano1979/project3/issues"
    },
    "homepage": "https://github.com/molano1979/project3#readme",
    "dependencies": {
        "concurrently": "^7.2.1",
        "dotenv": "^16.0.1",
        "google-maps-react": "^2.0.6",
        "watch": "^1.0.2"
    },
    "devDependencies": {
        "eslint": "^8.16.0",
        "eslint-config-airbnb": "^19.0.4",
        "eslint-plugin-import": "^2.26.0",
        "eslint-plugin-jsx-a11y": "^6.5.1",
        "eslint-plugin-react": "^7.30.0",
        "eslint-plugin-react-hooks": "^4.5.0"
    }
}
